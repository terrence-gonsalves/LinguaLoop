    "icon": "./assets/images/linguaLoopLogo.png",
    "scheme": "lingualoop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/linguaLoopLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
